# Copy to .env.deploy on target host and fill real values

OPENAI_API_KEY=
SUPERTONE_API_KEY=
SUPERTONE_API_KEY_1=
SUPERTONE_API_KEY_2=
SUPERTONE_API_KEY_3=
SUPERTONE_API_KEY_4=
SUPERTONE_API_KEY_5=
SUPERTONE_BASE_URL=https://supertoneapi.com
SUPERTONE_OUTPUT_FORMAT=mp3

MONGODB_URI=mongodb://mongodb:27017/ragdb
REDIS_HOST=redis
REDIS_PORT=6379
QDRANT_HOST=qdrant
QDRANT_PORT=6334
QDRANT_URL=http://qdrant:6333
QDRANT_COLLECTION=user_memories
MONITORING_PERSISTENT_ENABLED=true

# CORS 허용 오리진(쉼표 구분). 동일 오리진 배포면 비워둔다.
# 예: https://app.example.com,https://admin.example.com
WEB_CORS_ALLOWED_ORIGINS=

# Nginx /admin/monitoring 기본 인증
NGINX_BASIC_AUTH_USER=
NGINX_BASIC_AUTH_PASSWORD=
