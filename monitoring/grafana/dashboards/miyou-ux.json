    {
      "datasource": { "uid": "prometheus" },
      "description": "첫 응답까지 걸리는 시간 (Time To First Byte) 중앙값",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "orange", "value": 2000 },
              { "color": "red", "value": 4000 }
            ]
          },
          "unit": "ms"
        }
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "ux_response_latency_first{job=~\"$job\", instance=~\"$instance\", quantile=\"0.5\"}",
          "refId": "A"
        }
      ],
      "title": "평균 TTFB (Median Time To First Byte)",
      "type": "stat"
    },
    {
      "datasource": { "uid": "prometheus" },
