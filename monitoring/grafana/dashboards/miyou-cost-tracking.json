    {
      "datasource": { "uid": "prometheus" },
      "description": "일일 평균 비용 (이번 달 총 비용 / 일수)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "(llm_cost_usd_monthly{job=~\"$job\", instance=~\"$instance\"} + tts_cost_usd_monthly{job=~\"$job\", instance=~\"$instance\"}) / day_of_month()",
