      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 100 },
              { "color": "yellow", "value": 300 },
              { "color": "green", "value": 500 }
            ]
          },
          "unit": "currencyUSD",
          "min": 0,
          "max": 1000
