        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "orange", "value": 80 },
              { "color": "red", "value": 100 }
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "llm_cost_usd_daily{job=~\"$job\", instance=~\"$instance\"} + tts_cost_usd_daily{job=~\"$job\", instance=~\"$instance\"}",
          "refId": "A"
        }
      ],
      "title": "오늘 총 비용 (Today's Total Cost)",
