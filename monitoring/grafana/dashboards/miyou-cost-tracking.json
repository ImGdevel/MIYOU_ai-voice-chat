      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Total Cost" }]
      },
      "targets": [
        {
          "expr": "topk(10, sum(llm_cost_by_user_total{job=~\"$job\", instance=~\"$instance\"}) by (user_id, model))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "사용자별 비용 Top 10 (Top 10 Users by Cost)",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "user_id": "User ID",
              "model": "Model",
              "Value": "Total Cost"
            }
          }
        }
      ],
      "type": "table"
    }
