        }
      },
      "gridPos": { "h": 7, "w": 16, "x": 0, "y": 17 },
      "id": 7,
      "options": {
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum(rate(tts_cost_by_provider_total{job=~\"$job\", instance=~\"$instance\"}[5m])) by (provider) * 60",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "title": "TTS 비용 추이 (TTS Cost Over Time)",
      "type": "timeseries"
    },
    {
      "datasource": { "uid": "prometheus" },
      "description": "TTS 문자당 평균 비용",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "decimals": 6,
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 17 },
      "id": 8,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "sum(tts_cost_usd_total{job=~\"$job\", instance=~\"$instance\"}) / sum(tts_characters_total{job=~\"$job\", instance=~\"$instance\"})",
          "refId": "A"
        }
      ],
      "title": "문자당 비용 (Cost per Character)",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
      "id": 103,
      "title": "사용자별 비용 (Cost by User)",
      "type": "row"
    },
    {
      "datasource": { "uid": "prometheus" },
      "description": "사용자별 LLM 비용 상위 10명",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": { "align": "left", "displayMode": "auto" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 25 },
