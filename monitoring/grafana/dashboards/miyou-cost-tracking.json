        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "llm_cost_usd_monthly{job=~\"$job\", instance=~\"$instance\"} + tts_cost_usd_monthly{job=~\"$job\", instance=~\"$instance\"}",
          "refId": "A"
        }
