{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "TTS 엔드포인트 상태(Endpoint Health Status)",
      "type": "row"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "1": { "text": "HEALTHY", "color": "green" } } },
            { "type": "value", "options": { "0": { "text": "TEMP_FAIL", "color": "orange" } } },
            { "type": "value", "options": { "-1": { "text": "PERM_FAIL", "color": "red" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 18, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colWidth": 0.9,
        "legend": { "displayMode": "list", "placement": "bottom" },
        "rowHeight": 0.9,
        "showValue": "auto",
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "tts_endpoint_health{job=~\"$job\",instance=~\"$instance\"}",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "엔드포인트 상태 그리드(Health Status Grid)",
      "type": "status-history"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 3 },
              { "color": "green", "value": 5 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "count(tts_endpoint_health{job=~\"$job\",instance=~\"$instance\"} == 1)",
          "refId": "A"
        }
      ],
      "title": "정상 엔드포인트 수",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 101,
      "title": "요청 분포(Request Distribution) & 부하(Load)",
      "type": "row"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "orientation": "horizontal",
            "barWidth": 0.8,
            "showValue": "always"
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 8 },
      "id": 3,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "tts_endpoint_active_requests{job=~\"$job\",instance=~\"$instance\"}",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "엔드포인트별 활성 요청(Active Requests)",
      "type": "barchart"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 8 },
      "id": 4,
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value"] },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "displayLabels": ["percent"]
      },
      "targets": [
        {
          "expr": "sum(tts_endpoint_active_requests{job=~\"$job\",instance=~\"$instance\"}) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "요청 분포(Request Distribution) - 파이 차트",
      "type": "piechart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "id": 102,
      "title": "성능(Performance) & 가용성(Availability)",
      "type": "row"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 16 },
      "id": 5,
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "tts_endpoint_active_requests{job=~\"$job\",instance=~\"$instance\"}",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "활성 요청 추이(Active Requests Over Time)",
      "type": "timeseries"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.95 }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 16 },
      "id": 6,
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "avg_over_time(tts_endpoint_health{job=~\"$job\",instance=~\"$instance\"}[5m]) > 0",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "엔드포인트 가용성(Availability) - 5분 평균",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 103,
      "title": "상태 변화(Health Changes) & 로그(Logs)",
      "type": "row"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "id": 7,
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "changes(tts_endpoint_health{job=~\"$job\",instance=~\"$instance\"}[5m])",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "상태 변화(Health Status Changes) - 5분 윈도우",
      "type": "timeseries"
    },
    {
      "datasource": { "uid": "loki" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "id": 8,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "expr": "{job=\"docker-logs\", container=\"miyou-dialogue-app\"} |~ \"(?i)TTS|Supertone\" |~ \"(?i)error|exception|failed|timeout\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "TTS 에러 로그(Error Logs)",
      "type": "logs"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 104,
      "title": "엔드포인트 상세 테이블(Details Table)",
      "type": "row"
    },
    {
      "datasource": { "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [
            { "type": "value", "options": { "1": { "text": "HEALTHY", "color": "green" } } },
            { "type": "value", "options": { "0": { "text": "TEMP_FAIL", "color": "orange" } } },
            { "type": "value", "options": { "-1": { "text": "PERM_FAIL", "color": "red" } } }
          ]
        }
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 33 },
      "id": 9,
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Active Requests", "desc": true }]
      },
      "targets": [
        {
          "expr": "tts_endpoint_health{job=~\"$job\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "tts_endpoint_active_requests{job=~\"$job\",instance=~\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "B"
        }
      ],
      "title": "TTS 엔드포인트 상태 테이블(Status Table)",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "job": true, "instance": true, "__name__": true },
            "renameByName": {
              "endpoint": "엔드포인트(Endpoint)",
              "Value #A": "상태(Health Status)",
              "Value #B": "활성 요청(Active Requests)"
            }
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "15s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["miyou", "tts", "loadbalancer"],
