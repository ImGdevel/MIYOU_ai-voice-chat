      "targets": [
        {
          "expr": "sum(reactor_netty_http_server_connections_active{job=~\"$job\",instance=~\"$instance\"}) or sum(reactor_netty_connection_provider_active_connections{job=~\"$job\",instance=~\"$instance\"}) or vector(0)",
          "legendFormat": "활성 연결(Active Connections)",
          "refId": "A"
        },
        {
          "expr": "sum(reactor_netty_eventloop_pending_tasks{job=~\"$job\",instance=~\"$instance\"}) or vector(0)",
