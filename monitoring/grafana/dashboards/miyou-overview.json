          "expr": "sum(rate(http_server_requests_seconds_count{job=~\"$job\",instance=~\"$instance\",uri!~\"/actuator.*|/admin.*\",status=~\"2..\"}[1m]))",
          "legendFormat": "2xx",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=~\"$job\",instance=~\"$instance\",uri!~\"/actuator.*|/admin.*\",status=~\"3..\"}[1m]))",
          "legendFormat": "3xx",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=~\"$job\",instance=~\"$instance\",uri!~\"/actuator.*|/admin.*\",status=~\"4..\"}[1m]))",
          "legendFormat": "4xx",
          "refId": "C"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=~\"$job\",instance=~\"$instance\",uri!~\"/actuator.*|/admin.*\",status=~\"5..\"}[1m]))",
          "legendFormat": "5xx",
          "refId": "D"
        }
      ],
